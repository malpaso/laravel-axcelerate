# Axcelerate API Configuration
AXCELERATE_BASE_URL=https://your-domain.axcelerate.com
AXCELERATE_WS_TOKEN=your-web-service-token-here
AXCELERATE_API_TOKEN=your-api-token-here

# Optional Configuration
AXCELERATE_TIMEOUT=30
AXCELERATE_RETRY_ATTEMPTS=3
AXCELERATE_RETRY_DELAY=1000
AXCELERATE_LOG_REQUESTS=false

# Database Configuration (for testing)
DB_CONNECTION=sqlite
DB_DATABASE=:memory:

# Application Configuration
APP_NAME="Laravel Axcelerate"
APP_ENV=local
APP_KEY=base64:test-key-for-laravel-axcelerate-package
APP_DEBUG=true
APP_URL=http://localhost